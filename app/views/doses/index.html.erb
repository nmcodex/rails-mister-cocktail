<h1>Doses#index</h1>


<ul>
  <% @doses.each do |dose| %>
  <li>
    <%= dose.name %> <%= dose.ingredient.name %>
    <%= dose.description %>
    <p>Belongs to =<%= dose.cocktail.name %></p>
    <p><%= link_to "DELETE DOSE", dose_path(dose), method: :delete,
      data: { confirm: "Are you sure?" } %></p>
  </li>
  <% end %>
</ul>
